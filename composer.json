{
  "name": "templatemonster/healthchecks",
  "description": "HTTP and console health checks for microservices",
  "keywords": ["rest", "yii2", "microservices", "api"],
  "type": "package",
  "version": "1.0.0",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "indigerd\\rest\\": "src/rest",
      "templatemonster\\healthchecks\\": "src/healthchecks"
    }
  },
  "repositories":[
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "guzzlehttp/guzzle": "^7.3",
    "yiisoft/yii2": "^2.0.42",
    "indigerd/yii2-rest": "*"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "sort-packages": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
